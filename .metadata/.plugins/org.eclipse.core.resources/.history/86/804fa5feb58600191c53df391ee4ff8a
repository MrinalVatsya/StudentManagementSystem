 * 
 */
package com.mind.poc;

import java.util.ArrayList;

/**
 * @author mrinalvatsya
 *
 */
public class DiameterNArrayTree {
	
	ArrayList<ArrayList<Integer>> g;
	
	 public int solve(ArrayList<Integer> a) {
		 
		 int i = 0;
		 
		 int n = a.size();
		 g = new ArrayList<>(n);
		 
		 for(i = 0; i < n; i++)
		 {
			 g[i] = new ArrayList<>();
		 }
		 
		 for(i = 0; i < a.size(); i++)
		 {
			 int x = a.get(i);
			 if(x!=-1)
			 {
				 g[x]
			 }
		 }
	    }

}
