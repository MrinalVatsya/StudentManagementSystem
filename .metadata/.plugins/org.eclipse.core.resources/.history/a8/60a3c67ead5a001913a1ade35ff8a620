package com.mind.poc;

//This class represents a directed graph using adjacency list 
//representation 
public class TestClass 
{ 
	private static Boolean rec(int x)
	{
		if(ans == x)
			return true;
		int ans =  0;
		ans = ans + 1;
		return ans;
		
	}
	public static void main(String[] args) {
		System.out.println(rec());
	}
} 
