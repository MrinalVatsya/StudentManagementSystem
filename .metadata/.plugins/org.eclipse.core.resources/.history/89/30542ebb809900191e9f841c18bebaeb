package com.mind.poc;

/**
 * @author mrinalvatsya
 *
 */

class ListNode {
	public int val;
	public ListNode next;
	ListNode(int x) { val = x; next = null; }
}
public class InterviewBitLinkedList {


	public ListNode removeNthFromEnd(ListNode a, int b) {
		
		int count = 0;
		while(a != null)
		{
			count++;
			a = a.next;
		}

	}

}
