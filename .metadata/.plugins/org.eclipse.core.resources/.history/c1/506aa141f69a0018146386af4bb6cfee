package javademo;
import java.util.*;
class Subject
{
	String name;
	Integer actual_marks;
	Subject(String name, Integer actual_marks)
	{
		this.name = name;
		this.actual_marks = actual_marks;
	}
}
class Student
{
	Integer rollno;
	String name;
	Integer age;
	List<Subject> l = null;
	Student(Integer rollno, String name, Integer age)
	{
		this.rollno = rollno;
		this.name = name;
		this.age = age;
	}
}
